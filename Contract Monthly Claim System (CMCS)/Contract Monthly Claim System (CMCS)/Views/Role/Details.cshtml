@model Contract_Monthly_Claim_System__CMCS_.Models.Role
@{
    ViewData["Title"] = "Role Details";
}

<h2>Role Details</h2>

<div class="card">
    <div class="card-body">
        <dl class="row">
            <dt class="col-sm-2">Role ID</dt>
            <dd class="col-sm-10">@Model.RoleID</dd>

            <dt class="col-sm-2">Role Name</dt>
            <dd class="col-sm-10">@Model.RoleName</dd>
        </dl>
    </div>
</div>

<div class="mt-3">
    <a asp-action="Edit" asp-route-id="@Model.RoleID" class="btn btn-warning">Edit</a>
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</div>